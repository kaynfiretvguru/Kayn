<addon id="plugin.program.kaynswizard" name="Kayn's Wizard" version="1.1.5" provider-name="Kayn">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>program executable</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">A simple Wizard to assist in downloading and installing media from the Kayn Repository.</summary>
    <description lang="en">This is a easy to use Wizard to help users download and install builds, Add-Ons, dependencies, updates, and more from the Kayn Repository.</description>
	<email>kaynfiretvguru@gmail.com</email>
    <platform>all</platform>
  </extension>
</addon>

    <addon id="repository.kayn" name="Kayn's Repository" version="1.1.3" provider-name="Kayn">
        <requires>
            <import addon="xbmc.addon" version="12.0.0"/>
        </requires>
        <extension point="xbmc.addon.repository" name="Kayn's Repository">
            <info compressed="false">https://raw.githubusercontent.com/kaynfiretvguru/Kayn/master/_repo/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/kaynfiretvguru/Kayn/master/_repo/addons.xml.md5</checksum>
            <datadir zip="true">https://raw.githubusercontent.com/kaynfiretvguru/Kayn/master/_repo/</datadir>
            <hashes>false</hashes>
        </extension>
        <extension point="xbmc.addon.metadata">
            <summary>A Simple Repository</summary>
            <description>Repository for media and dependencies hosted and/or created by Kayn.</description>
            <platform>all</platform>
    </extension>
</addon>

